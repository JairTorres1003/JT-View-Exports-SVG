{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON schema for i18n in View Exports SVG (client) extension for Visual Studio Code",
  "type": "object",
  "properties": {
    "errors": {
      "type": "object",
      "properties": {
        "FailedToFindTheRootElement": {
          "type": "string",
          "default": "Failed to find the root element"
        },
        "UnknownError": {
          "type": "string",
          "default": "Unknown error"
        },
        "TheComponentIsEmpty": {
          "type": "string",
          "default": "The component is empty"
        },
        "VSCodeApiIsNotAvailable": {
          "type": "string",
          "default": "VSCode API is not available"
        },
        "NoResponse": {
          "type": "string",
          "default": "No response"
        },
        "NoMessageTypeProvided": {
          "type": "string",
          "default": "No message type provided"
        },
        "FailedToReloadEditor": {
          "type": "string",
          "default": "Failed to reload editor"
        },
        "FailedToUpdateUserConfiguration": {
          "type": "string",
          "default": "Failed to update user configuration"
        },
        "FailedToUpdateUserKeybindings": {
          "type": "string",
          "default": "Failed to update user keybindings"
        },
        "EditorReferenceIsNotAvailable": {
          "type": "string",
          "default": "Editor reference is not available"
        },
        "FailedToActivateExtensionTheme": {
          "type": "string",
          "default": "Failed to activate extension theme"
        }
      },
      "required": [
        "FailedToFindTheRootElement",
        "UnknownError",
        "TheComponentIsEmpty",
        "VSCodeApiIsNotAvailable",
        "NoResponse",
        "NoMessageTypeProvided",
        "FailedToReloadEditor",
        "FailedToUpdateUserConfiguration",
        "FailedToUpdateUserKeybindings",
        "EditorReferenceIsNotAvailable",
        "FailedToActivateExtensionTheme"
      ]
    },
    "labels": {
      "type": "object",
      "properties": {
        "Loading": {
          "type": "string",
          "default": "Loading..."
        },
        "ExtractingComponents": {
          "type": "string",
          "default": "Extracting components..."
        },
        "Search": {
          "type": "string",
          "default": "Search"
        },
        "Clear": {
          "type": "string",
          "default": "Clear"
        },
        "Copied {{value}} to clipboard": {
          "type": "string",
          "default": "Copied <b>{{value}}</b> to clipboard"
        },
        "Close": {
          "type": "string",
          "default": "Close"
        }
      },
      "required": [
        "Loading",
        "ExtractingComponents",
        "Search",
        "Clear",
        "Copied {{value}} to clipboard",
        "Close"
      ]
    },
    "pickers": {
      "type": "object",
      "properties": {
        "color": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "default": "Select a color"
            }
          },
          "required": ["title"]
        }
      },
      "required": ["color"]
    },
    "DevTools": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "default": "DevTools"
        },
        "playground": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "default": "Playground"
            },
            "HideCode": {
              "type": "string",
              "default": "Hide code"
            },
            "ShowCode": {
              "type": "string",
              "default": "Show code"
            },
            "CopyCode": {
              "type": "string",
              "default": "Copy code"
            },
            "ResetCode": {
              "type": "string",
              "default": "Reset code"
            },
            "MoreActions": {
              "type": "string",
              "default": "More actions..."
            }
          },
          "required": ["title", "HideCode", "ShowCode", "CopyCode", "ResetCode", "MoreActions"]
        },
        "menu": {
          "type": "object",
          "properties": {
            "ReloadPlayground": {
              "type": "string",
              "default": "Reload playground"
            },
            "ReloadEditor": {
              "type": "string",
              "default": "Reload editor"
            }
          },
          "required": ["ReloadPlayground", "ReloadEditor"]
        }
      },
      "required": ["title", "playground", "menu"]
    }
  },
  "required": ["errors", "labels", "pickers", "DevTools"]
}
