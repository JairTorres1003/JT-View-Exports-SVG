{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON schema for i18n in View Exports SVG (client) extension for Visual Studio Code",
  "type": "object",
  "properties": {
    "labels": {
      "type": "object",
      "properties": {
        "clear": {
          "type": "string",
          "default": "Clear"
        },
        "search": {
          "type": "string",
          "default": "Search"
        },
        "copy": {
          "type": "string",
          "default": "Copy"
        },
        "copyToClipboard": {
          "type": "string",
          "default": "Copy to clipboard"
        },
        "copied {{value}} to clipboard": {
          "type": "string",
          "pattern": ".*{{value}}.*",
          "patternErrorMessage": "The message must contain the variable {{value}}",
          "default": "Copied {{value}} to clipboard"
        },
        "loading": {
          "type": "string",
          "default": "Loading..."
        },
        "extractingComponents": {
          "type": "string",
          "default": "Extracting components..."
        }
      },
      "required": [
        "clear",
        "search",
        "copy",
        "copyToClipboard",
        "copied {{value}} to clipboard",
        "loading",
        "extractingComponents"
      ]
    },
    "errors": {
      "type": "object",
      "properties": {
        "VSCode API is not available.": {
          "type": "string",
          "default": "VSCode API is not available."
        }
      },
      "required": ["VSCode API is not available."]
    }
  },
  "required": ["labels", "errors"]
}
