{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "JSON schema for i18n in View Exports SVG (client) extension for Visual Studio Code",
  "type": "object",
  "properties": {
    "labels": {
      "type": "object",
      "properties": {
        "Clear": {
          "type": "string",
          "default": "Clear"
        },
        "Close": {
          "type": "string",
          "default": "Close"
        },
        "Search": {
          "type": "string",
          "default": "Search"
        },
        "Select color": {
          "type": "string",
          "default": "Select color"
        },
        "Copy": {
          "type": "string",
          "default": "Copy"
        },
        "Copy to clipboard": {
          "type": "string",
          "default": "Copy to clipboard"
        },
        "Copied {{value}} to clipboard": {
          "type": "string",
          "default": "Copied <b>{{value}}</b> to clipboard"
        },
        "Loading": {
          "type": "string",
          "default": "Loading..."
        },
        "Extracting components": {
          "type": "string",
          "default": "Extracting components..."
        }
      },
      "required": [
        "Clear",
        "Close",
        "Search",
        "Select color",
        "Copy",
        "Copy to clipboard",
        "Copied {{value}} to clipboard",
        "Loading",
        "Extracting components"
      ]
    },
    "DevTools": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string",
          "default": "DevTools"
        }
      },
      "required": ["title"]
    },
    "errors": {
      "type": "object",
      "properties": {
        "VSCode API is not available": {
          "type": "string",
          "default": "VSCode API is not available"
        }
      },
      "required": ["VSCode API is not available"]
    }
  },
  "required": ["labels", "errors"]
}
