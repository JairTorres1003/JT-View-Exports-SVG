{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": ["--extensionDevelopmentPath=${workspaceFolder}"],
      "outFiles": ["${workspaceFolder}/out/cjs/**/*.js"]
    },
    {
      "name": "Extension Tests",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}",
        "--extensionTestsPath=${workspaceFolder}/out/cjs/test/suite/index",
        "${workspaceFolder}/workspaceTest.code-workspace"
      ],
      "outFiles": ["${workspaceFolder}/out/cjs/test/**/*.js"],
      "preLaunchTask": "TaskTest"
    }
  ]
}
