{
  "name": "jt-view-exports-svg",
  "displayName": "%extension.title%",
  "description": "%extension.description%",
  "keywords": [
    "Extension",
    "Icon assets",
    "Icon management",
    "Javascript",
    "JSX",
    "React",
    "SVG icons",
    "TSX",
    "TypeScript",
    "Visual Studio Code",
    "VSCode"
  ],
  "private": true,
  "workspaces": ["packages/*"],
  "license": "MIT",
  "author": "Jair Torres",
  "publisher": "JairTorres1003",
  "version": "4.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JairTorres1003/JT-View-Exports-SVG.git"
  },
  "bugs": {
    "url": "https://github.com/JairTorres1003/JT-View-Exports-SVG/issues"
  },
  "homepage": "https://github.com/JairTorres1003/JT-View-Exports-SVG#readme",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": ["Other"],
  "icon": "icon.png",
  "main": "./out/cjs/extension.js",
  "browser": "./out/cjs/web/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "JT-View-Exports-SVG.showMenu",
        "title": "%extension.commands.showMenu.title%",
        "icon": {
          "dark": "./assets/JT View Exports SVG - ICON.svg",
          "light": "./assets/JT View Exports SVG - ICON.svg"
        }
      },
      {
        "command": "JT-View-Exports-SVG.scanning",
        "title": "%extension.commands.scanning.title%",
        "icon": "$(search)",
        "category": "View Exports SVG"
      },
      {
        "command": "JT-View-Exports-SVG.reloadTheme",
        "title": "%extension.commands.reloadTheme.title%",
        "icon": "$(sync)",
        "category": "View Exports SVG",
        "enablement": "activeWebviewPanelId == 'JT-View-Exports-SVG'"
      },
      {
        "command": "JT-View-Exports-SVG.collapseAll",
        "title": "%extension.commands.collapseAll.title%",
        "icon": "$(collapse-all)",
        "category": "View Exports SVG",
        "enablement": "activeWebviewPanelId == 'JT-View-Exports-SVG' && JT-View-Exports-SVG.isExpanded && JT-View-Exports-SVG.showActions"
      },
      {
        "command": "JT-View-Exports-SVG.expandAll",
        "title": "%extension.commands.expandAll.title%",
        "icon": "$(expand-all)",
        "category": "View Exports SVG",
        "enablement": "activeWebviewPanelId == 'JT-View-Exports-SVG' && !JT-View-Exports-SVG.isExpanded && JT-View-Exports-SVG.showActions"
      },
      {
        "command": "JT-View-Exports-SVG.openDevTools",
        "title": "%extension.commands.openDevTools.title%",
        "icon": "$(jt-viewExportsSVG-tools-filled)",
        "category": "View Exports SVG",
        "enablement": "activeWebviewPanelId == 'JT-View-Exports-SVG' && !JT-View-Exports-SVG.isOpenDevTools && JT-View-Exports-SVG.showActions"
      },
      {
        "command": "JT-View-Exports-SVG.closeDevTools",
        "title": "%extension.commands.closeDevTools.title%",
        "icon": "$(jt-viewExportsSVG-tools)",
        "category": "View Exports SVG",
        "enablement": "activeWebviewPanelId == 'JT-View-Exports-SVG' && JT-View-Exports-SVG.isOpenDevTools && JT-View-Exports-SVG.showActions"
      },
      {
        "command": "JT-View-Exports-SVG.clearCache",
        "title": "%extension.commands.clearCache.title%",
        "icon": "$(trash)",
        "category": "View Exports SVG"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "JT-View-Exports-SVG.showMenu",
          "when": "resourceLangId =~ /^(javascript|typescript|javascriptreact|typescriptreact)$/ && resourceFilename =~ /^(?!.*\\.d\\.ts$).*\\.(ts|tsx|js|jsx)$/"
        }
      ],
      "editor/context": [
        {
          "command": "JT-View-Exports-SVG.showMenu",
          "when": "resourceLangId =~ /^(javascript|typescript|javascriptreact|typescriptreact)$/ && resourceFilename =~ /^(?!.*\\.d\\.ts$).*\\.(ts|tsx|js|jsx)$/"
        }
      ],
      "editor/title": [
        {
          "command": "JT-View-Exports-SVG.collapseAll",
          "group": "navigation@1",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG' && JT-View-Exports-SVG.isExpanded && JT-View-Exports-SVG.showActions"
        },
        {
          "command": "JT-View-Exports-SVG.expandAll",
          "group": "navigation@1",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG' && !JT-View-Exports-SVG.isExpanded && JT-View-Exports-SVG.showActions"
        },
        {
          "command": "JT-View-Exports-SVG.openDevTools",
          "group": "navigation@2",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG' && !JT-View-Exports-SVG.isOpenDevTools && JT-View-Exports-SVG.showActions"
        },
        {
          "command": "JT-View-Exports-SVG.closeDevTools",
          "group": "navigation@2",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG' && JT-View-Exports-SVG.isOpenDevTools && JT-View-Exports-SVG.showActions"
        },
        {
          "command": "JT-View-Exports-SVG.clearCache",
          "group": "navigation@3",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG'"
        }
      ]
    },
    "configuration": [
      {
        "title": "%extension.title%",
        "properties": {
          "JT-View-Exports-SVG.defaultExpandAll": {
            "type": "boolean",
            "default": false,
            "title": "%extension.config.defaultExpandAll.title%",
            "description": "%extension.config.defaultExpandAll.description%",
            "scope": "application"
          },
          "JT-View-Exports-SVG.defaultClickToOpenDevTools": {
            "type": "boolean",
            "default": true,
            "title": "%extension.config.defaultClickToOpenDevTools.title%",
            "description": "%extension.config.defaultClickToOpenDevTools.description%",
            "scope": "application"
          },
          "JT-View-Exports-SVG.assetsPath": {
            "type": "array",
            "default": [],
            "title": "%extension.config.assetsPath.title%",
            "description": "%extension.config.assetsPath.description%"
          },
          "JT-View-Exports-SVG.ignoreDirectories": {
            "type": "array",
            "default": [],
            "title": "%extension.config.ignoreDirectories.title%",
            "description": "%extension.config.ignoreDirectories.description%",
            "items": {
              "type": "string"
            }
          },
          "JT-View-Exports-SVG.lastScanDate": {
            "type": "string",
            "default": "",
            "title": "%extension.config.lastScanDate.title%",
            "description": "%extension.config.lastScanDate.description%",
            "readOnly": true
          },
          "JT-View-Exports-SVG.defaultIconProperties": {
            "type": "object",
            "title": "%extension.config.defaultIconProperties.title%",
            "description": "%extension.config.defaultIconProperties.description%",
            "default": {
              "className": "",
              "color": "#0a0a0a",
              "fill": "#0a0a0a",
              "height": "28",
              "size": "28",
              "state": "false",
              "stroke": "#0a0a0a",
              "strokeColor": "#0a0a0a",
              "strokeWidth": "0.2",
              "style": "{}",
              "sx": "{}",
              "width": "28"
            },
            "additionalProperties": {
              "type": "string"
            }
          },
          "JT-View-Exports-SVG.showNotExportedIcons": {
            "type": "boolean",
            "default": false,
            "title": "%extension.config.showNotExportedIcons.title%",
            "description": "%extension.config.showNotExportedIcons.description%"
          },
          "JT-View-Exports-SVG.groupPatterns": {
            "type": "object",
            "title": "%extension.config.groupPatterns.title%",
            "markdownDescription": "%extension.config.groupPatterns.description%",
            "default": {},
            "additionalProperties": {
              "type": "string"
            }
          },
          "JT-View-Exports-SVG.recentIcons.showIcons": {
            "type": "boolean",
            "default": true,
            "scope": "application",
            "title": "%extension.config.recentIcons.showIcons.title%",
            "description": "%extension.config.recentIcons.showIcons.description%",
            "order": 0
          },
          "JT-View-Exports-SVG.recentIcons.limitShowIcons": {
            "type": "number",
            "default": 10,
            "title": "%extension.config.recentIcons.limitShowIcons.title%",
            "description": "%extension.config.recentIcons.limitShowIcons.description%",
            "minimum": 1,
            "maximum": 50,
            "multipleOf": 1,
            "scope": "application",
            "order": 1
          }
        }
      }
    ],
    "icons": {
      "jt-viewExportsSVG-tools": {
        "description": "View Exports SVG Tools Icon",
        "default": {
          "fontPath": "./assets/fonts/icon_moon.woff",
          "fontCharacter": "\\e900"
        }
      },
      "jt-viewExportsSVG-tools-filled": {
        "description": "View Exports SVG Tools Filled Icon",
        "default": {
          "fontPath": "./assets/fonts/icon_moon.woff",
          "fontCharacter": "\\e901"
        }
      }
    }
  },
  "scripts": {
    "install:all": "npm install && cd client && npm install",
    "biome:check": "biome check --write",
    "biome:format": "biome format --write",
    "clean": "rimraf ./out",
    "compile:root": "tsc -b $TS_FILE && tsc-alias -p $TS_FILE",
    "compile": "TS_FILE=./tsconfig.cjs.json npm run compile:root",
    "compile:esm": "TS_FILE=./tsconfig.esm.json npm run compile:root",
    "compile:esbuild": "node esbuild.config.js --production",
    "compile:esbuild:dev": "node esbuild.config.js",
    "compile:esbuild:web": "node esbuild.config.js --production --web-mode",
    "compile:esbuild:web:dev": "node esbuild.config.js --web-mode",
    "watch:root": "concurrently \"tsc -w --sourceMap -p $TS_FILE\" \"tsc-alias -w -p $TS_FILE\"",
    "watch": "TS_FILE=./tsconfig.cjs.json npm run watch:root",
    "watch:esm": "TS_FILE=./tsconfig.esm.json npm run watch:root",
    "watch:esbuild": "node esbuild.config.js --watch",
    "watch:esbuild:web": "node esbuild.config.js --watch --web-mode",
    "test": "node ./out/cjs/test/runTest.js --verbose",
    "pretest": "npm-run-all clean client:build compile",
    "vscode:prepublish": "npm-run-all biome:check test clean compile:esbuild compile:esbuild:web",
    "client:build": "cd client && npm run build",
    "client:dev": "cd client && npm run dev",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. $FOLDER_EXAMPLE",
    "prepare-readme": "node prepare-readme.js",
    "prepare-readme:restore": "node prepare-readme.js --restore",
    "run:package": "vsce package",
    "package": "npm-run-all prepare-readme run:package prepare-readme:restore",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/types": "^7.29.0",
    "@biomejs/biome": "^2.4.2",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/babel__traverse": "^7.28.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "22.19.11",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.109.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/test-web": "^0.0.79",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.3",
    "glob": "^13.0.6",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.3",
    "sinon": "^21.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@jt-view-exports-svg/core": "workspace:*",
    "lodash": "^4.17.23",
    "minimatch": "^10.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "badges": [
    {
      "url": "https://vsmarketplacebadges.dev/version/jairtorres1003.jt-view-exports-svg.png",
      "description": "Visual Studio Marketplace Version",
      "href": "https://marketplace.visualstudio.com/items?itemName=JairTorres1003.jt-view-exports-svg"
    },
    {
      "url": "https://vsmarketplacebadges.dev/installs-short/jairtorres1003.jt-view-exports-svg.png",
      "description": "Visual Studio Marketplace Installs",
      "href": "https://marketplace.visualstudio.com/items?itemName=JairTorres1003.jt-view-exports-svg"
    },
    {
      "url": "https://vsmarketplacebadges.dev/downloads-short/jairtorres1003.jt-view-exports-svg.png",
      "description": "Visual Studio Marketplace Downloads",
      "href": "https://marketplace.visualstudio.com/items?itemName=JairTorres1003.jt-view-exports-svg"
    },
    {
      "url": "https://vsmarketplacebadges.dev/rating-star/jairtorres1003.jt-view-exports-svg.png",
      "description": "Visual Studio Marketplace Rating",
      "href": "https://marketplace.visualstudio.com/items?itemName=JairTorres1003.jt-view-exports-svg"
    }
  ],
  "galleryBanner": {
    "color": "#166c7b",
    "theme": "dark"
  }
}
