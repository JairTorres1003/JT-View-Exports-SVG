{
  "name": "jt-view-exports-svg-monorepo",
  "displayName": "View Exports SVG",
  "description": "ðŸš€ The ultimate tool to explore and manage SVG components directly in Visual Studio Code.",
  "keywords": [
    "Extension",
    "Icon assets",
    "Icon management",
    "Javascript",
    "JSX",
    "React",
    "SVG icons",
    "TSX",
    "TypeScript",
    "Visual Studio Code",
    "VSCode"
  ],
  "version": "4.1.0",
  "private": true,
  "license": "MIT",
  "author": "Jair Torres",
  "repository": {
    "type": "git",
    "url": "https://github.com/JairTorres1003/JT-View-Exports-SVG.git"
  },
  "bugs": {
    "url": "https://github.com/JairTorres1003/JT-View-Exports-SVG/issues"
  },
  "homepage": "https://github.com/JairTorres1003/JT-View-Exports-SVG#readme",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.30.1",
  "scripts": {
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "typecheck": "pnpm -r --parallel typecheck",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "clean": "pnpm -r -F '!@jt-view-exports-svg/{docs,webview}' run clean",
    "build:core": "pnpm -F @jt-view-exports-svg/core run build",
    "test:extension": "pnpm -F jt-view-exports-svg run test",
    "clean:extension": "pnpm -F jt-view-exports-svg run clean",
    "build:webview": "pnpm -F @jt-view-exports-svg/webview run build",
    "package:extension": "pnpm -F jt-view-exports-svg run package",
    "prepare-readme": "node scripts/prepare-readme.js",
    "prerelease": "npm-run-all lint prepare-readme",
    "release": "npm-run-all clean build:core test:extension clean:extension build:webview package:extension",
    "postrelease": "node scripts/prepare-readme.js --restore"
  },
  "devDependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@babel/types": "^7.29.0",
    "@biomejs/biome": "2.4.2",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@types/babel__traverse": "^7.28.0",
    "@types/lodash": "^4.17.23",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.19.11",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.109.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/test-web": "^0.0.79",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.3",
    "glob": "^13.0.6",
    "husky": "^9.1.7",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.3",
    "sinon": "^21.0.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@jt-view-exports-svg/core": "workspace:*",
    "lodash": "^4.17.23",
    "minimatch": "^10.2.2"
  }
}
