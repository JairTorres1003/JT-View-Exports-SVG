{
  "name": "jt-view-exports-svg",
  "displayName": "%extension.title%",
  "description": "%extension.description%",
  "keywords": [
    "Extension",
    "Icon assets",
    "Icon management",
    "Javascript",
    "JSX",
    "React",
    "SVG icons",
    "TSX",
    "TypeScript",
    "Visual Studio Code",
    "VSCode"
  ],
  "private": true,
  "license": "MIT",
  "author": "Jair Torres",
  "publisher": "JairTorres1003",
  "version": "4.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/JairTorres1003/JT-View-Exports-SVG.git"
  },
  "bugs": {
    "url": "https://github.com/JairTorres1003/JT-View-Exports-SVG/issues"
  },
  "homepage": "https://github.com/JairTorres1003/JT-View-Exports-SVG#readme",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "icon": "icon.png",
  "main": "./out/cjs/extension.js",
  "l10n": "./l10n",
  "contributes": {
    "commands": [
      {
        "command": "JT-View-Exports-SVG.showMenu",
        "title": "%extension.commands.showMenu.title%",
        "icon": {
          "dark": "./assets/JT View Exports SVG - ICON.svg",
          "light": "./assets/JT View Exports SVG - ICON.svg"
        }
      },
      {
        "command": "JT-View-Exports-SVG.collapseAll",
        "title": "%extension.commands.collapseAll.title%",
        "icon": "$(collapse-all)"
      },
      {
        "command": "JT-View-Exports-SVG.expandAll",
        "title": "%extension.commands.expandAll.title%",
        "icon": "$(expand-all)"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "JT-View-Exports-SVG.showMenu",
          "when": "resourceLangId =~ /^(javascript|typescript|javascriptreact|typescriptreact)$/ && resourceFilename =~ /^(?!.*\\.d\\.ts$).*\\.(ts|tsx|js|jsx)$/"
        }
      ],
      "editor/context": [
        {
          "command": "JT-View-Exports-SVG.showMenu",
          "when": "resourceLangId =~ /^(javascript|typescript|javascriptreact|typescriptreact)$/ && resourceFilename =~ /^(?!.*\\.d\\.ts$).*\\.(ts|tsx|js|jsx)$/"
        }
      ],
      "editor/title": [
        {
          "command": "JT-View-Exports-SVG.collapseAll",
          "group": "navigation",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG' && JT-View-Exports-SVG.isExpanded"
        },
        {
          "command": "JT-View-Exports-SVG.expandAll",
          "group": "navigation",
          "when": "activeWebviewPanelId == 'JT-View-Exports-SVG' && !JT-View-Exports-SVG.isExpanded"
        }
      ]
    },
    "configuration": [
      {
        "title": "%extension.title%",
        "properties": {
          "JT-View-Exports-SVG.defaultExpandAll": {
            "type": "boolean",
            "default": false,
            "title": "%extension.config.defaultExpandAll.title%",
            "description": "%extension.config.defaultExpandAll.description%",
            "scope": "application"
          },
          "JT-View-Exports-SVG.assetsPath": {
            "type": "array",
            "default": [],
            "title": "%extension.config.assetsPath.title%",
            "description": "%extension.config.assetsPath.description%"
          },
          "JT-View-Exports-SVG.ignoreDirectories": {
            "type": "array",
            "default": [],
            "title": "%extension.config.ignoreDirectories.title%",
            "description": "%extension.config.ignoreDirectories.description%",
            "items": {
              "type": "string"
            }
          },
          "JT-View-Exports-SVG.lastScanDate": {
            "type": "string",
            "default": "",
            "title": "%extension.config.lastScanDate.title%",
            "description": "%extension.config.lastScanDate.description%",
            "readOnly": true
          },
          "JT-View-Exports-SVG.defaultIconProperties": {
            "type": "object",
            "title": "%extension.config.defaultIconProperties.title%",
            "description": "%extension.config.defaultIconProperties.description%",
            "default": {
              "className": "",
              "color": "#0a0a0a",
              "fill": "#0a0a0a",
              "height": "28",
              "size": "28",
              "state": "false",
              "stroke": "#0a0a0a",
              "strokeColor": "#0a0a0a",
              "strokeWidth": "0.2",
              "style": "{}",
              "sx": "{}",
              "width": "28"
            },
            "additionalProperties": {
              "type": "string"
            }
          },
          "JT-View-Exports-SVG.showNotExportedIcons": {
            "type": "boolean",
            "default": false,
            "title": "%extension.config.showNotExportedIcons.title%",
            "description": "%extension.config.showNotExportedIcons.description%"
          },
          "JT-View-Exports-SVG.groupPatterns": {
            "type": "object",
            "title": "%extension.config.groupPatterns.title%",
            "markdownDescription": "%extension.config.groupPatterns.description%",
            "default": {},
            "additionalProperties": {
              "type": "string"
            }
          },
          "JT-View-Exports-SVG.recentIcons.showIcons": {
            "type": "boolean",
            "default": true,
            "scope": "application",
            "title": "%extension.config.recentIcons.showIcons.title%",
            "description": "%extension.config.recentIcons.showIcons.description%",
            "order": 0
          },
          "JT-View-Exports-SVG.recentIcons.limitShowIcons": {
            "type": "number",
            "default": 10,
            "title": "%extension.config.recentIcons.limitShowIcons.title%",
            "description": "%extension.config.recentIcons.limitShowIcons.description%",
            "minimum": 1,
            "maximum": 50,
            "multipleOf": 1,
            "scope": "application",
            "order": 1
          }
        }
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && cd client && npm install",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix --cache",
    "format:check": "prettier --check \"**/*.{ts,json,md,yml,cjs}\"",
    "format": "prettier --write \"**/*.{ts,json,md,yml,cjs}\"",
    "compile:root": "tsc -b $npm_config_ts_file && if [ \"$npm_config_copy_interfaces\" = \"true\" ]; then cp -r ./src/interfaces $(jq -r .compilerOptions.outDir $npm_config_ts_file)/interfaces; fi && tsc-alias -p $npm_config_ts_file",
    "compile": "npm run compile:root --ts-file='./tsconfig.cjs.json'",
    "compile:esm": "npm run compile:root --ts-file='./tsconfig.esm.json' --copy-interfaces=true",
    "compile:types": "npm run compile:root --ts-file='./tsconfig.types.json' --copy-interfaces=true",
    "compile:clean": "rimraf ./out",
    "watch:root": "concurrently \"tsc -w --sourceMap -p $npm_config_ts_file\" \"tsc-alias -w -p $npm_config_ts_file\"",
    "watch": "npm run watch:root --ts-file='./tsconfig.cjs.json'",
    "watch:esm": "npm run watch:root --ts-file='./tsconfig.esm.json'",
    "test": "node ./out/cjs/test/runTest.js",
    "pretest": "npm-run-all compile:clean client:build compile",
    "vscode:prepublish": "npm-run-all lint test",
    "client:lint": "cd client && npm run lint",
    "client:lint:fix": "cd client && npm run lint:fix",
    "client:format:check": "cd client && npm run format:check",
    "client:format": "cd client && npm run format",
    "client:build": "cd client && npm run build",
    "client:dev": "cd client && npm run dev",
    "prepare": "husky"
  },
  "devDependencies": {
    "@babel/types": "^7.26.3",
    "@types/babel__traverse": "^7.20.6",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vscode/l10n-dev": "^0.0.35",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "minimatch": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sinon": "^19.0.2",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.4",
    "lodash": "^4.17.21"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
